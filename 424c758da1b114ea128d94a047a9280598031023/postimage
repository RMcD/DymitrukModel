<component id="193" name="Context" componentClassID="{BCEFE59B-6819-47F7-A125-63753B33ABB7}" description="OLE DB Source" localeId="-1" usesDispositions="true" validateExternalMetadata="True" version="7" pipelineVersion="0" contactInfo="OLE DB Source;Microsoft Corporation; Microsoft SqlServer v10; (C) Microsoft Corporation; All Rights Reserved; http://www.microsoft.com/sql/support;7">
<properties>
<property id="194" name="CommandTimeout" dataType="System.Int32" state="default" isArray="false" description="The number of seconds before a command times out.  A value of 0 indicates an infinite time-out." typeConverter="" UITypeEditor="" containsID="false" expressionType="None">0</property>
<property id="195" name="OpenRowset" dataType="System.String" state="default" isArray="false" description="Specifies the name of the database object used to open a rowset." typeConverter="" UITypeEditor="" containsID="false" expressionType="None"></property>
<property id="196" name="OpenRowsetVariable" dataType="System.String" state="default" isArray="false" description="Specifies the variable that contains the name of the database object used to open a rowset." typeConverter="" UITypeEditor="" containsID="false" expressionType="None"></property>
<property id="197" name="SqlCommand" dataType="System.String" state="default" isArray="false" description="The SQL command to be executed." typeConverter="" UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=10.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91" containsID="false" expressionType="None">SELECT 'Through ' + MAX(edfi.TermType.CodeValue) + ' ' + CAST(academicRecord.SchoolYear AS VARCHAR(4)) As Context, SCA.SchoolId
FROM edfi.StudentAcademicRecord AS academicRecord
INNER JOIN
            edfi.TermType ON academicRecord.TermTypeId = edfi.TermType.TermTypeId
Inner Join edfi.CurrentStudentSchoolAssociation SCA
On academicRecord.StudentUSI = SCA.StudentUSI
WHERE SchoolYear = (SELECT MAX(SchoolYear) FROM edfi.StudentAcademicRecord 
					WHERE SchoolYear NOT IN 
						(SELECT SchoolYear FROM edfi.SchoolYearType WHERE CurrentSchoolYear = 'True'))
GROUP BY academicRecord.SchoolYear, SCA.SchoolId
Order by SCA.SchoolId</property>
