<property id="182" name="SqlCommand" dataType="System.String" state="default" isArray="false" description="The SQL command to be executed." typeConverter="" UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=10.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91" containsID="false" expressionType="None">Select MAX(G.GradingPeriodTypeId) As GradingPeriodTypeId, 
G.SchoolId, SchoolYear from edfi.Grade G

Inner Join edfi.GradingPeriodType GPT
On GPT.GradingPeriodTypeId = G.GradingPeriodTypeId
and GPT.GradingPeriodTypeId NOT IN (7,8,9)

Group by G.SchoolId, SchoolYear
Order by G.SchoolId</property>
