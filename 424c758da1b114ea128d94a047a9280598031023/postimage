from  edfi.StaffEducationOrgEmploymentAssociation SE
Inner Join edfi.TeacherSchoolAssociation TCA
On SE.StaffUSI = TCA.StaffUSI
WHERE SE.HireDate &lt;= (SELECT MAX(Date) FROM edfi.CalendarDate)
Group by TCA.SchoolId
Order by TCA.SchoolId</property>
