	case when MetricId = 1617 then 1616
		 when MetricId = 1618 then 1616
		 when MetricId = 1619 then 1616
		 when MetricId = 1620  then 1616
		 when MetricId = 1621 then 1616
		 when MetricId = 1622 then 1616
		 when MetricId = 1623 then 1616
		 when MetricId = 1624 then 1616
		 when MetricId = 1625 then 1616
		 when MetricId = 1626 then 1616
		 when MetricId = 1627 then 1616
		 when MetricId = 1628 then 1616
		 when MetricId = 1629 then 1616
		 when MetricId = 1630 then 1616
		 when MetricId = 1631 then 1616
		 

	end as MetricId
	
 from metric.MetricInstance CM 
 inner join domain.SchoolMetricInstanceSet SE 
	ON CM.MetricInstanceSetKey=SE.MetricInstanceSetKey
	
 Inner Join domain.SchoolInformation CI 
	on SE.SchoolId = CI.SchoolId 
	
 where CM.MetricId in (1617,1618,1619,1620,1621,1622,1623,1624,1625,1626,1627,1628,1629,1630,1631)
Order by SE.SchoolId</property>
