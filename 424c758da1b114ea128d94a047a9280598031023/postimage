<component id="70" name="Failing Grades, Grades Below C, Falling Grades, Algebra I" componentClassID="{BCEFE59B-6819-47F7-A125-63753B33ABB7}" description="OLE DB Source" localeId="1033" usesDispositions="true" validateExternalMetadata="True" version="7" pipelineVersion="0" contactInfo="OLE DB Source;Microsoft Corporation; Microsoft SqlServer v10; (C) Microsoft Corporation; All Rights Reserved; http://www.microsoft.com/sql/support;7">
<properties>
<property id="71" name="CommandTimeout" dataType="System.Int32" state="default" isArray="false" description="The number of seconds before a command times out.  A value of 0 indicates an infinite time-out." typeConverter="" UITypeEditor="" containsID="false" expressionType="None">0</property>
<property id="72" name="OpenRowset" dataType="System.String" state="default" isArray="false" description="Specifies the name of the database object used to open a rowset." typeConverter="" UITypeEditor="" containsID="false" expressionType="None"></property>
<property id="73" name="OpenRowsetVariable" dataType="System.String" state="default" isArray="false" description="Specifies the variable that contains the name of the database object used to open a rowset." typeConverter="" UITypeEditor="" containsID="false" expressionType="None"></property>
<property id="74" name="SqlCommand" dataType="System.String" state="default" isArray="false" description="The SQL command to be executed." typeConverter="" UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=10.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91" containsID="false" expressionType="None">Select distinct SE.SchoolId ,CM.MetricInstanceSetKey,

     case 
     when MetricId = 271 then 270
     when MetricId = 272 then 270
     when MetricId = 1442 then 1283
     when MetricId = 1443 then 1283
     when MetricId = 1275 then 270
     when MetricId = 1276 then 270
     when MetricId = 1278 then 270
     when MetricId = 1279 then 270
     when MetricId = 1281 then 270
     when MetricId = 1282 then 270

     when MetricId = 263 then 262
     when MetricId = 264 then 262
     when MetricId = 265 then 262

     when MetricId = 267 then 266
     when MetricId = 268 then 266
     when MetricId = 269 then 266
     when MetricId = 414 then 266

     when MetricId = 1063 then 1062
     when MetricId = 1064 then 1062
     when MetricId = 1065 then 1062
     when MetricId = 1066 then 1062
     when MetricId = 1067 then 1062

end as MetricId
from metric.MetricInstance CM 
inner join domain.SchoolMetricInstanceSet SE ON
SE.MetricInstanceSetKey=CM.MetricInstanceSetKey

where CM.MetricId in (271, 272, 1442, 1443, 1275, 1276, 1278, 1279, 1281, 1282, 263, 264, 265, 267, 268, 269, 414, 1063, 1064, 1065, 1066, 1067)
Order by SE.SchoolId</property>
