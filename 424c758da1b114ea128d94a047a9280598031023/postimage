case when MetricId = 420 then 261
        when MetricId = 421 then 261
        when MetricId = 422 then 261
        when MetricId = 423 then 261



        
end as MetricId

from metric.MetricInstance CM 
inner join domain.SchoolMetricInstanceSet SE on
CM.MetricInstanceSetKey=SE.MetricInstanceSetKey
where CM.MetricId in (420, 421, 422, 423)
Order by SE.SchoolId</property>
