from  edfi.LeaveEvent LE
Inner Join edfi.TeacherSchoolAssociation TCA
On LE.StaffUSI = TCA.StaffUSI
WHERE LE.EventDate &lt;= (SELECT MAX(Date) FROM edfi.CalendarDate)
Group by TCA.SchoolId
Order by TCA.SchoolId</property>
