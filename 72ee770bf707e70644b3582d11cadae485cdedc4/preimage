<property id="68" name="CommandTimeout" dataType="System.Int32" state="default" isArray="false" description="The number of seconds before a command times out.  A value of 0 indicates an infinite time-out." typeConverter="" UITypeEditor="" containsID="false" expressionType="None">0</property>
<property id="69" name="OpenRowset" dataType="System.String" state="default" isArray="false" description="Specifies the name of the database object used to open a rowset." typeConverter="" UITypeEditor="" containsID="false" expressionType="None"></property>
<property id="70" name="OpenRowsetVariable" dataType="System.String" state="default" isArray="false" description="Specifies the variable that contains the name of the database object used to open a rowset." typeConverter="" UITypeEditor="" containsID="false" expressionType="None"></property>
<property id="71" name="SqlCommand" dataType="System.String" state="default" isArray="false" description="The SQL command to be executed." typeConverter="" UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=10.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91" containsID="false" expressionType="None">Select AA.SchoolId,AA.PeriodTypeId,AA.BeginDay,AA.EndDay,AA.NumberOfDays,
CASE 
	WHEN AA.GradingPeriodTypeId = 1 THEN (CAST(AA.GradingPeriodTypeId As Varchar )+'st Grading Period')
	WHEN AA.GradingPeriodTypeId = 2 THEN (CAST(AA.GradingPeriodTypeId As Varchar )+'nd Grading Period')
	WHEN AA.GradingPeriodTypeId = 3 THEN  (CAST(AA.GradingPeriodTypeId As Varchar )+'rd Grading Period')
	ELSE CAST(AA.GradingPeriodTypeId As Varchar )+'th Grading Period'
	END AS PeriodName, PeriodSequence
from 
(
Select distinct CD.EducationOrganizationId as Schoolid, 
( 3) as PeriodTypeId,
edfi.GradingPeriodType.periodsequence as GradingPeriodTypeId, edfi.GradingPeriodType.periodsequence,
<<<<<<<
=======
YEAR(CD.Date) as SchoolYear,
>>>>>>>
MIN(CD.Date) as BeginDay,
MAX(CD.Date) as EndDay,
COUNT(CD.Date) as NumberOfDays	
from edfi.CalendarDate CD,  edfi.GradingPeriod GP
inner join edfi.GradingPeriodType on gp.GradingPeriodTypeId = edfi.GradingPeriodType.GradingPeriodTypeId
where CD.CalendarEventTypeId &lt;&gt; 3 and PeriodSequence in (1,2,3,4,5,6)
and CD.EducationOrganizationId = GP.EducationOrganizationId
and CD.Date between GP.BeginDate and GP.EndDate
<<<<<<<
group by CD.EducationOrganizationId,YEAR(CD.Date),edfi.GradingPeriodType.periodsequence
=======
group by CD.EducationOrganizationId,edfi.GradingPeriodType.periodsequence
>>>>>>>
) as AA

order by BeginDay</property>
