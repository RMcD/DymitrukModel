<property id="35" name="CommandTimeout" dataType="System.Int32" state="default" isArray="false" description="The number of seconds before a command times out.  A value of 0 indicates an infinite time-out." typeConverter="" UITypeEditor="" containsID="false" expressionType="None">0</property>
<property id="36" name="OpenRowset" dataType="System.String" state="default" isArray="false" description="Specifies the name of the database object used to open a rowset." typeConverter="" UITypeEditor="" containsID="false" expressionType="None"></property>
<property id="37" name="OpenRowsetVariable" dataType="System.String" state="default" isArray="false" description="Specifies the variable that contains the name of the database object used to open a rowset." typeConverter="" UITypeEditor="" containsID="false" expressionType="None"></property>
<property id="38" name="SqlCommand" dataType="System.String" state="default" isArray="false" description="The SQL command to be executed." typeConverter="" UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=10.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91" containsID="false" expressionType="None">Select AA.SchoolId,AA.PeriodTypeId,AA.BeginDay,AA.EndDay,AA.NumberOfDays,
CAST((CAST(AA.SchoolYear As Varchar )+' Fall Semester') as varchar(44)) As PeriodName
from 
(
Select distinct CD.EducationOrganizationId as Schoolid, 
( 5) as PeriodTypeId,
YEAR(CD.Date) + 1 as SchoolYear,
MIN(CD.Date) as BeginDay,
MAX(CD.Date) as EndDay,
COUNT(CD.Date) as NumberOfDays	
from edfi.CalendarDate CD
where CD.CalendarEventTypeId &lt;&gt; 3 and DATEPART(MONTH,CD.Date) in (8,9,10,11,12)
group by CD.EducationOrganizationId,YEAR(CD.Date)
) as AA


UNION ALL

Select AA.SchoolId,AA.PeriodTypeId,AA.BeginDay,AA.EndDay,AA.NumberOfDays,
cast((CAST(AA.SchoolYear As Varchar )+' Spring Semester') as varchar (44)) As PeriodName
from 
(
Select distinct CD.EducationOrganizationId as Schoolid, 
( 5) as PeriodTypeId,
YEAR(CD.Date) as SchoolYear,
MIN(CD.Date) as BeginDay,
MAX(CD.Date) as EndDay,
COUNT(CD.Date) as NumberOfDays	
from edfi.CalendarDate CD
where CD.CalendarEventTypeId &lt;&gt; 3 and DATEPART(MONTH,CD.Date) in (1,2,3,4,5)
group by CD.EducationOrganizationId,YEAR(CD.Date)
) as AA</property>
